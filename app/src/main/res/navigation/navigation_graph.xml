<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/eventsFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.treadstone.grpproject.daropointsevents.fragments.Welcome"
        android:label="WelcomeFragment"
        tools:layout="@layout/fragment_welcome" />
    <fragment
        android:id="@+id/eventsFragment"
        android:name="com.treadstone.grpproject.daropointsevents.fragments.Events"
        android:label="EventsFragment"
        tools:layout="@layout/fragment_events" />
    <fragment
        android:id="@+id/apply_events"
        android:name="com.treadstone.grpproject.daropointsevents.fragments.ApplyEvents"
        android:label="SimilarEventsFragment"
        tools:layout="@layout/fragment_apply_events" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="org.fossasia.openevent.general.search.SearchFragment"
        android:label="SearchFragment"
        tools:layout="@layout/fragment_search" >
        <argument
            android:name="@string/saved_date"
            app:argType="string" />
    </fragment>


</navigation>